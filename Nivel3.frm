Attribute VB_Name = "Nivel3"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit

Dim Aciertos As Integer
Dim Intentos As Integer
Dim Color As Integer
Dim Tiempo As Integer
Dim RndColor(5) As Long

'Genera un color RGB aleatorio
Public Function ColorRandom() As Long
    ColorRandom = RGB( _
        Int(Rnd() * 256), _
        Int(Rnd() * 256), _
        Int(Rnd() * 256))
End Function

Private Sub btnNiveles_Click()
    Niveles.Show
    Unload Me
    Set Nivel3 = Nothing
End Sub

Private Sub Form_Load()
    Dim i As Integer
    Dim j As Integer
    
    Tiempo = 0
    Timer1.Enabled = False
    Timer1.Interval = 1000
    
    'Crea 6 colores aleatorios que se usaran en el juego
    For i = 0 To 5
        RndColor(i) = ColorRandom()
        pctColor(i).BackColor = RndColor(i)
    Next i
    
    'Asigna un color aleatorio de los 6 generados a cada cuadro en la mitad izquierda de la pantalla
    For i = 0 To 49
        Randomize
        pctCuadro(i).BackColor = RndColor(Int(6 * Rnd))
    Next i
End Sub

Private Sub Frame1_MouseMove(Button As Integer, Shift As Integer, X As Single, Y As Single)
    labControles.ForeColor = vbWhite
End Sub

Private Sub labControles_Click()
    MsgBox ("Mirá, los controles del juego son muy intuitivos, pero voy a asumir que presionaste este botón por error y decirtelo de todas maneras: tenés que agarrar un color y arrastrarlo a la casilla que espeje dicho color en el lado derecho. Listo, andá a jugar ahora")
    labControles.ForeColor = vbWhite
End Sub

Private Sub labControles_MouseMove(Button As Integer, Shift As Integer, X As Single, Y As Single)
    labControles.ForeColor = vbRed
End Sub

Private Sub pctColor_MouseDown(Index As Integer, Button As Integer, Shift As Integer, X As Single, Y As Single)
    pctColor(Index).Drag vbBeginDrag
    Color = Index
End Sub

Private Sub pctCuadro_DragDrop(Index As Integer, Source As Control, X As Single, Y As Single)
    Dim i, j, k As Integer

    pctCuadro(Index).Drag vbEndDrag
    
    If Timer1.Enabled = False Then Timer1.Enabled = True
    
    If Index >= 90 Then
        If (pctCuadro((90 - Index) * -1).BackColor = pctColor(Color).BackColor) Then
            pctCuadro(Index).BackColor = pctCuadro((90 - Index) * -1).BackColor
            Aciertos = Aciertos + 1
        End If
    ElseIf Index >= 80 And Index < 90 Then
            If (pctCuadro(Index - 70).BackColor = pctColor(Color).BackColor) Then
                pctCuadro(Index).BackColor = pctCuadro(Index - 70).BackColor
                Aciertos = Aciertos + 1
            End If
    ElseIf Index >= 70 And Index < 80 Then
            If (pctCuadro(Index - 50).BackColor = pctColor(Color).BackColor) Then
                pctCuadro(Index).BackColor = pctCuadro(Index - 50).BackColor
                Aciertos = Aciertos + 1
            End If
    ElseIf Index >= 60 And Index < 70 Then
            If (pctCuadro(Index - 30).BackColor = pctColor(Color).BackColor) Then
                pctCuadro(Index).BackColor = pctCuadro(Index - 30).BackColor
                Aciertos = Aciertos + 1
            End If
    ElseIf Index >= 50 And Index < 60 Then
            If (pctCuadro(Index - 10).BackColor = pctColor(Color).BackColor) Then
                pctCuadro(Index).BackColor = pctCuadro(Index - 10).BackColor
                Aciertos = Aciertos + 1
            End If
    End If
    
    Intentos = Intentos + 1
    
    labAciertos.Caption = Aciertos
    labIntentos.Caption = Intentos
    
    Dim Resp As Integer
    
    If Aciertos >= 50 Then
        Resp = MsgBox("Felicitaciones, completaste el Nivel 3 y ganaste el juego. Presiona OK para ver los creditos!", vbOKOnly, "Nivel Completado")
        'Carga los créditos
        Creditos.Show
        Unload Me
        Set Nivel3 = Nothing
    End If
End Sub

Private Sub Timer1_Timer()
    Tiempo = Tiempo + 1
    labTiempo.Caption = Tiempo
End Sub

Private Function coso()
    Select Case Index
       'COLOR 1
       '------------------------------------------------------------
        Case 0
            If Color = 0 Then
